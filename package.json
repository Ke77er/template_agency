  },
  "dependencies": {
    "location": "^0.0.1",
    "navigator": "^1.0.1",
    "vue": "^2.7.16",
    "vue-template-compiler": "^2.7.16"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^13.3.0",
    "browserify-hmr": "^0.3.7",
    "cross-env": "^7.0.3",
    "envify": "^3.4.1",
    "http-server": "^0.9.0",
    "node-sass": "^9.0.0",
    "npm-run-all": "^2.3.0",
    "terser": "^5.28.1",
    "uglify-js": "^2.8.29",
    "vueify": "^9.4.1",
    "watchify": "^3.11.1"
  },
  "browserify": {
    "transform": [
      "vueify",
      "babelify"
    ]
  },
